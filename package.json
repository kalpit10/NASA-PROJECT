  "dependencies": {
    "axios": "^1.3.5",
    "concurrently": "^7.6.0",
    "core-js": "^3.30.1",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5"
  }
}
